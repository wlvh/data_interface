{
  "additionalProperties": false,
  "description": "变换执行过程中的日志记录。",
  "properties": {
    "x-spec-version": {
      "default": "2.0.0",
      "description": "契约对象对应的 Schema 版本号，用于离线回放与迁移。",
      "title": "X-Spec-Version",
      "type": "string"
    },
    "level": {
      "description": "日志级别。",
      "minLength": 1,
      "title": "Level",
      "type": "string"
    },
    "message": {
      "description": "日志内容。",
      "minLength": 1,
      "title": "Message",
      "type": "string"
    },
    "timestamp": {
      "description": "UTC 时间的日志时间戳。",
      "format": "date-time",
      "title": "Timestamp",
      "type": "string"
    }
  },
  "required": [
    "level",
    "message",
    "timestamp"
  ],
  "title": "TransformLog",
  "type": "object",
  "$id": "https://schemas.data-interface.local/contracts/transform_log.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "version": "2.0.0"
}
